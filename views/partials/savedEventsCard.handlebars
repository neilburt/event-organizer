
<div class="card">
  <div class="card-body">
    <h3 class="card-title" id="event-title">{{title}}</h3>
    <p class="card-text" id="event-type">{{type}}</p>
    <p class="card-text" id="event-date">{{datetime_local}}</p>
    <a href={{url}} target="_blank" class="btn btn-primary">Link to the Event!</a>
    <button class="deleteBtn btn btn-primary" data-id="{{id}}">Delete event</button>
  </div>
</div>
